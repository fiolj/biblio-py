  journal = {Physical Review Letters},
  year = {2008},
  volume = {100},
  abstract = {The recent observation of a pressure-induced insulator to metal (I-M)
    transition in pure LaMnO$_{3}$ [I. Loa , Phys. Rev. Lett. 87,
    125501 (2001).PRLTAO0031-900710.1103/PhysRevLett.87.125501], opens
