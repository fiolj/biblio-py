@ARTICLE{Pellenq2008JCP_4701,
  author = { Pellenq, Roland J.-M.  and  Marinelli, Francis  and  Fuhr, Javier D.  and  Fernandez-Alonso, Felix  and  Refson, Keith },
  title = {Strong physisorption site for H$_{2}$ in K- and Li-doped porous
